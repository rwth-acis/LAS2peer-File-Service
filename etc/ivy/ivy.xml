<ivy-module version="2.0">
    <info organisation="i5" module="las2peer-service"/>
    <publications>
		<artifact type="pom" ext="pom" conf="default"/>
		<artifact type="jar" ext="jar" conf="default"/>
	</publications>
    <dependencies>
        <dependency org="i5" name="las2peer" rev="0.3-SNAPSHOT" />		 
        <dependency org="i5" name="las2peer-web-connector" rev="0.3-SNAPSHOT"/>
        <!-- json parsing and writing -->
        <dependency org="org.glassfish" name="javax.json" rev="1.0.4"/>
        <!-- database implementation -->
        <dependency org="com.h2database" name="h2" rev="1.4.192"/>
        <!-- database migration, see https://flywaydb.org -->
        <dependency org="org.flywaydb" name="flyway-core" rev="4.0.3"/>
    </dependencies>
</ivy-module>
